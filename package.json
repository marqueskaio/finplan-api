{
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "prisma": "^4.13.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.2.3"
  },
  "name": "finplan",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.cjs",
    "test:unit-verb": "npm test -- --watch -c jest-unit-config.cjs --verbose --silent=false",
    "test:integration": "npm test -- --watch -c jest-integration-config.cjs",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "db:generate": "cd node_modules/.prisma && prisma generate",
    "db:push": "cd node_modules/.prisma && prisma db push",
    "db:test:generate": "dotenv -e .env.test prisma generate",
    "db:test:push": "dotenv -e .env.test prisma db push"
  },
