datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}


generator client {
  provider = "prisma-client-js"
}

model FaturasModel {
  id                    Int     @id @default(autoincrement())
  mes_fatura            String 
  dia_do_fechamento     String
  dia_do_vencimento     String

  @@map("faturas")
}