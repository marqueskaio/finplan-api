generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model FaturasModel {
  id                Int    @id @default(autoincrement())
  mes_fatura        String
  banco             Bancos
  dia_do_fechamento DateTime
  dia_do_vencimento String
  
}

enum Bancos {
  NUBANK
  INTER
  C6
  ITAU
  BRADESCO
  SANTANDER
}
